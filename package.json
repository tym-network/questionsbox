{
  "name": "questions-box",
  "description": "Create a set of questions and record your friends answering them",
  "version": "0.0.4",
  "author": "Th√©otime Loiseau <contac@theotimeloiseau.com>",
  "license": "GPL-3.0",
  "homepage": "https://github.com/tym-network/questionsbox",
  "main": "web/js/main.js",
  "scripts": {
    "start": "electron .",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer:dev\"",
    "build-main": "webpack --config webpack.main.js",
    "build-renderer:dev": "webpack --config webpack.dev.js",
    "build-renderer:prod": "webpack --config webpack.prod.js",
    "build:release": "node ./scripts/build.js",
    "watch": "concurrently \"npm run watch-main\" \"npm run watch-renderer\"",
    "watch-main": "webpack --watch --config webpack.main.js",
    "watch-renderer": "webpack --watch --config webpack.dev.js"
  },
  "dependencies": {
    "i18next": "^11.10.2",
    "react-bootstrap": "^0.32.4",
    "react-select": "^2.4.2",
    "react-transition-group": "^2.3.1",
    "recordrtc": "^5.4.8",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "electron": "^3.0.7",
    "electron-builder": "20.31.2",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.10.0",
    "webpack-clean-obsolete-chunks": "^0.4.0",
    "webpack-merge": "^4.1.2"
  }
}
