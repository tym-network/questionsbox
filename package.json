{
  "name": "questions-box",
  "description": "Create a set of questions and record your friends answering them",
  "version": "0.0.4",
  "author": "Th√©otime Loiseau <contac@theotimeloiseau.com>",
  "license": "GPL-3.0",
  "homepage": "https://github.com/tym-network/questionsbox",
  "main": "web/js/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tym-network/questionsbox.git"
  },
  "scripts": {
    "start": "electron .",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer:dev\"",
    "build-main": "webpack --config webpack.main.js",
    "build-renderer:dev": "webpack --config webpack.dev.js",
    "build-renderer:prod": "webpack --config webpack.prod.js",
    "build:release": "node ./scripts/build.js",
    "watch": "concurrently \"npm run watch-main\" \"npm run watch-renderer\"",
    "watch-main": "webpack --watch --config webpack.main.js",
    "watch-renderer": "webpack --watch --config webpack.dev.js"
  },
  "dependencies": {
    "i18next": "^11.10.2",
    "mem": "^5.1.0",
    "moment": "^2.24.0",
    "react-bootstrap": "^0.32.4",
    "react-select": "^2.4.2",
    "react-transition-group": "^2.3.1",
    "recordrtc": "^5.5.8",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^3.0.0",
    "electron": "^3.0.7",
    "electron-builder": "20.31.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "webpack": "^4.35.0",
    "webpack-clean-obsolete-chunks": "^0.4.0",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.1"
  }
}
