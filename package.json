{
    "name": "questions-box",
    "description": "Create a set of questions and record your friends answering them",
    "version": "0.0.6",
    "author": "Th√©otime Loiseau <contac@theotimeloiseau.com>",
    "license": "GPL-3.0",
    "homepage": "https://github.com/tym-network/questionsbox",
    "main": "web/js/main.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/tym-network/questionsbox.git"
    },
    "scripts": {
        "install-wsl": "npm install && npm uninstall electron && export npm_config_platform=win32 && npm install electron && unset npm_config_platform",
        "start": "electron .",
        "build:dev": "concurrently \"npm run build-main:dev\" \"npm run build-renderer:dev\"",
        "build:prod": "concurrently \"npm run build-main:prod\" \"npm run build-renderer:prod\"",
        "build-main:dev": "webpack --config webpack.main.dev.js",
        "build-main:prod": "webpack --config webpack.main.prod.js",
        "build-renderer:dev": "webpack --config webpack.renderer.dev.js",
        "build-renderer:prod": "webpack --config webpack.renderer.prod.js",
        "build:release": "node ./scripts/build.js",
        "start-dev-server": "webpack-dev-server --config webpack.renderer.dev.js",
        "watch": "concurrently \"npm run watch-main\" \"npm run start-dev-server\"",
        "watch-main": "webpack --watch --config webpack.main.dev.js",
        "watch-renderer": "webpack --watch --config webpack.renderer.dev.js"
    },
    "dependencies": {
        "electron-devtools-installer": "^3.2.0",
        "i18next": "^21.3.3",
        "lodash.memoize": "^4.1.2",
        "mem": "^9.0.1",
        "moment": "^2.29.1",
        "prop-types": "^15.7.2",
        "react": "^16.14.0",
        "react-bootstrap": "^2.0.0",
        "react-dom": "^16.14.0",
        "react-hot-loader": "^4.12.21",
        "react-select": "^5.1.0",
        "recordrtc": "^5.6.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.14.5",
        "@hot-loader/react-dom": "^17.0.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.3",
        "concurrently": "^6.3.0",
        "css-loader": "^3.6.0",
        "electron": "^9.1.0",
        "electron-builder": "22.13.1",
        "eslint": "^7.5.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.26.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^6.0.1",
        "sass-loader": "^10.2.0",
        "style-loader": "^1.2.1",
        "webpack": "^4.43.0",
        "webpack-clean-obsolete-chunks": "^0.4.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.8.0"
    }
}
